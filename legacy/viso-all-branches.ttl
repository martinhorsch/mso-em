##############  (1) agent ########################################

viso:agent a owl:Class;
  rdfs:subClassOf evmpo:interpreter;
  rdfs:comment "An entity (individual, group, institution) that can potentially act on a virtual marketplace."^^xs:string;
  owl:equivalentClass evmpo:agent.

####

# NOTE: EVMPO and VICO have a much richer sublcass structure for "agent", this
# is a simpflied version for what is needed here, and that is redundant
# if EVMPO/VICO are loaded

viso:human_agent rdfs:subClassOf viso:agent;
  owl:equivalentClass evmpo:human_agent.

viso:institution rdfs:subClassOf viso:agent;
  owl:disjointWith viso:human_agent;
  owl:equivalentClass evmpo:institution.
  
##############  (2) software  ####################################

viso:software a owl:Class;
  rdfs:comment "A computer program. Can be a software tool, a compiler (lower level, closer to the hardware) or an operating system."^^xs:string;
  rdfs:subClassOf evmpo:software_infrastructure;
  rdfs:subClassOf evmpo:information_content_entity.
  
viso:software_tool a owl:Class; 
  rdfs:subClassOf viso:software.

viso:compiler a owl:Class; 
  rdfs:subClassOf viso:software.

viso:operating_system a owl:Class; 
  rdfs:subClassOf viso:software.

viso:software owl:disjointUnionOf (viso:software_tool viso:compiler viso:operating_system). 

viso:windows a owl:Class; 
  rdfs:subClassOf viso:operating_system. 

viso:linux a owl:Class; 
  rdfs:subClassOf viso:operating_system.

viso:macos a owl:Class; 
  rdfs:subClassOf viso:operating_system.

viso:operating_system owl:disjointUnionOf (viso:windows viso:linux viso:macos).

viso:simulation_engine a owl:Class;
   rdfs:subClassOf viso:software_tool;
   rdfs:comment "A tool that solves the governing equations."^^xs:string.
   
viso:pre_processing_tool a owl:Class;
   rdfs:subClassOf viso:software_tool;
   rdfs:comment "A tool that helps in setting up a simulation (e.g., to generate the initial configuration)."^^xs:string.

viso:post_processing_tool a owl:Class;
   rdfs:subClassOf viso:software_tool;
   rdfs:comment """A tool that processes the raw data obtained solving the governing equations. 
   		Note that this splitting between solving and processing is a logical one (cf. RoMM),
		it does not necessarily imply the existence of a separate piece of software. 
		In fact, some of these post-processing functionalities are often embedded in the simulation engine itself 
		(e.g., the calculation of the system energy in a MD simulation is typically done on the fly by the solver itself)."""^^xs:string.

viso:visualizer a owl:Class;
   rdfs:subClassOf viso:post_processing_tool;
      rdfs:comment "A tool to read and visualize data (with no changes or additions to the data, cf. RoMM)."^^xs:string.

viso:converter a owl:Class;
   rdfs:subClassOf viso:post_processing_tool;
      rdfs:comment """A tool that changes the data without altering the physical state itself
                (e.g., it changes the numerical representation of a state, cf. RoMM)."""^^xs:string.

viso:extractor a owl:Class;
   rdfs:subClassOf viso:post_processing_tool;
      rdfs:comment """A tool that enriches the dataset/state description by further information 
      		   calculated based on the information already contained in the state. 
		   Examples include averaging, interpolation and the calculation of physical quantities (cf. RoMM)."""^^xs:string.

viso:software_tool owl:unionOf (viso:simulation_engine viso:pre_processing_tool  viso:post_processing_tool).

##############  (3a) license and (3b) license clause  ####################################

viso:license a owl:Class;
  rdfs:comment "Regulation of the right to use, modify and distribute something, in this case software."^^xs:string;
  rdfs:comment "Software license, as characterized in the Software Ontology (SWO)"^^xs:string;
  rdfs:subClassOf evmpo:legal_document;
  owl:equivalentClass swo:SWO_0000002. # <-> SWO 'software licence' 
swo:SWO_0000002 rdfs:subClassOf evmpo:legal_document.

viso:license_clause a owl:Class;
  rdfs:comment "License clause, as characterized in the Software Ontology (SWO)"^^xs:string;
  rdfs:subClassOf evmpo:information_content_entity;
  owl:equivalentClass swo:SWO_9000005. # <-> SWO 'licence clause' 
swo:SWO_9000005 rdfs:subClassOf evmpo:information_content_entity.

